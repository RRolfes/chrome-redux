{
  "manifest_version": 2,
  "name": "Chrome extension with React/Redux",
  "background":{
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "declarativeContent",
    "activeTab",
    "storage",
    "identity",
    "https://www.facebook.com/",
    "tabs"
  ],
  "version": "1.5"
}
